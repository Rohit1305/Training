package com.aurionpro.model;

public class TataFactory implements ICarfactory {
	private static ICarfactory tatafactory;

	private TataFactory() {
	}

	public static ICarfactory getMahindraInstance() {
		if (tatafactory == null) {
			return new TataFactory();
		}
		return tatafactory;
	}

	@Override
	public ICars makeCar() {
		return new Mahindra();
	}

}
